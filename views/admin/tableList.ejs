<% layout('layouts/base') -%>
  <div class="row">
<div class="col-md-12 col-sm-12 col-xs-12">
                <div class="x_panel">
                  <div class="x_title">
                    <h2>Table List</h2>
                    <div class="clearfix"></div>
                  </div>
                  <div class="x_title">
                    <a href="/admin/createtable" class="btn btn-success btn-xs pull-right">Add New</a>
                  </div>
                  
                  <div class="x_content">
                    <div class="table-responsive">
                        <p id="date_filter">
                            <span id="date-label-from" class="date-label">From </span>:<input type="text" id="min" name="min" class="date_range_filter date">&nbsp;&nbsp;&nbsp;<span id="date-label-to" class="date-label">To </span>:<input type="text" id="max" name="max" class="date_range_filter date">
                        </p>
                      <table id="datatable-checkbox" class="table table-striped table-bordered bulk_action">
                        <thead>
                          <tr class="headings">
                            <th>
                              <input type="checkbox" id="check-all" class="flat">
                            </th>
                            <th class="column-title1">Table Name </th>
                            <th class="column-title1">Bet Amount </th>
                            <th class="column-title1">Percentage</th>
                            <th class="column-title1">Target Lines</th>
                            <th class="column-title1">Table Type</th>
                            <th class="column-title1">Created</th>
                            
                            <th class="column-title1 no-link last"><span class="nobr">Action</span>
                            </th>
                            <th class="bulk-action">                             
                            </th>
                          </tr>
                        </thead>
                        <tbody>
                          <%
                          if(tableList.length >0){
                            tableList.forEach(function(tbl){ %>
                          
                          <tr class="even pointer">
                            <td class="a-center ">
                              <input type="checkbox" class="flat" name="table_records[]" value="<%=tbl.id%>">
                            </td>
                            <td class=" "><%=tbl.name%></td>
                            <td class=" "><%=tbl.bet%></td>
                            <td class=" "><%=tbl.percentage%></td>
                            <td class=" "><%=tbl.targetlines%></td>
                            <td class=" "><%=tbl.tabletype%></td>
                            <td class=" "><%=new Date(tbl.created_at).toDateString()%></td>
                             <td><a href="/admin/edittable/<%=tbl.id%>"><i class="fa fa-edit"></i> Edit</a></td>
                            <td class="last"><button type="button" class="btn btn-danger btn-xs deltbl" data-id="<%=tbl.id%>" data-vl="0">Delete</button>
                            </td>
                          </tr>
                          <% }) }%>
                        </tbody>
                      </table>
                    </div>
							
						
                  </div>
                </div>
              </div>
          </div>