<% layout('layouts/base') -%>
  <div class="row">
<div class="col-md-12 col-sm-12 col-xs-12">
                <div class="x_panel">
                  <div class="x_title">
                    <h2>User's Details</h2>
                    <div class="clearfix"></div>
                  </div>

                  <div class="x_content">
                    <div class="table-responsive">
                        <p id="date_filter" style="display: none;">
                            <span id="date-label-from" class="date-label">From </span>:<input type="text" id="min" name="min" class="date_range_filter date">&nbsp;&nbsp;&nbsp;<span id="date-label-to" class="date-label">To </span>:<input type="text" id="max" name="max" class="date_range_filter date">
                        </p>
                      <table id="datatable-checkbox" class="table table-striped table-bordered bulk_action">
                        <thead>
                          <tr class="headings">
                            <th class="column-title1">Username </th>
                            <th class="column-title1">Spent Amount</th>
                            <th class="column-title1">Spent For</th>
                            <th class="column-title1">Spent Date</th>
                          </tr>
                        </thead>

                        <tbody>
                          <%
                          if(logs.length >0){
                           logs.forEach(function(log){ %>
                          
                          <tr class="even pointer">
                            <td class=" "><%=log.username%></td>
                            <td class=" "><%=log.amount%></td>
                            <td class=" "><%=log.spent_for%></td>
                            <td class=" "><%=new Date(log.created_at).toDateString()%></td>
                          </tr>
                          <% }) }%>
                        </tbody>
                      </table>
                    </div>
							
						
                  </div>
                </div>
              </div>
          </div>